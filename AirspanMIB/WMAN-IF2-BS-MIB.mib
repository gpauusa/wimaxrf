WmanIf2BsSsStartupStatisticsEntry ::= SEQUENCE {
        wmanIf2BsSsAuthenAttempt                Counter64, 
        wmanIf2BsSsAuthenSuccess                Counter64,
        wmanIf2BsSsAuthenSuccessRate            INTEGER,
        wmanIf2BsSsRangingAttempt               Counter64, 

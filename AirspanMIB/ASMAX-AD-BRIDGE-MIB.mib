			DESCRIPTION
				"This table, together with asDot1adIntPepPortTable, contains the per-port provisioning
				information for the internal ports used when the bridge is operating in Provider-mode 
				(i.e. supports 802.1ad) and the SS port is configured to operate as a CEP port.
				This table concentrates principally on the operations applied to traffic entering the
				S-Vlan entity from the C-Vlan entity.
				The table is read-create to allow NMS to provision port configuration before the 
				address is learnt.
				"
			::= { asDot1adPortObjects 6 }

		
		-- 1.3.6.1.4.1.989.1.16.5.4.2.6.1
		asDot1adIntCvidRegistrationProvEntry OBJECT-TYPE
			SYNTAX AsDot1adIntCvidRegistrationProvEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"One row per air-interface, SS port MAC address, C-VID."
			INDEX { ifIndex, asDot1adIntCvidRegistrationProvAddress, asDot1adIntCvidRegistrationProvCvid }
			::= { asDot1adIntCvidRegistrationProvTable 1 }

		
		AsDot1adIntCvidRegistrationProvEntry ::=
			SEQUENCE { 
				asDot1adIntCvidRegistrationProvAddress
					MacAddress,
				asDot1adIntCvidRegistrationProvCvid
					AsDot1adVlanId,
				asDot1adIntCvidRegistrationProvSvid
					AsDot1adVlanIdOrNone,
				asDot1adIntCvidRegistrationProvUserPriority
					Integer32,
				asDot1adIntCvidRegistrationProvPepUntagged
					AsmaxBooleanSwitch,
				asDot1adIntCvidRegistrationProvCepUntagged
					AsmaxBooleanSwitch,
				asDot1adIntCvidRegistrationProvRowStatus
					RowStatus
			 }

		-- 1.3.6.1.4.1.989.1.16.5.4.2.6.1.1
		asDot1adIntCvidRegistrationProvAddress OBJECT-TYPE
			SYNTAX MacAddress
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"MAC Address identifying the port. The port here is the virtual SS air interface port."
			::= { asDot1adIntCvidRegistrationProvEntry 1 }

		
		-- 1.3.6.1.4.1.989.1.16.5.4.2.6.1.2
		asDot1adIntCvidRegistrationProvCvid OBJECT-TYPE
			SYNTAX AsDot1adVlanId
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"The C-VID, the customer Vlan, for which the Service Vlan (S-Vlan) tag to insert is 
				being defined.
				"
			REFERENCE
				"IEEE 802.1Q/D11 Section 12.10.1.1"
			::= { asDot1adIntCvidRegistrationProvEntry 2 }

		
		-- 1.3.6.1.4.1.989.1.16.5.4.2.6.1.3
		asDot1adIntCvidRegistrationProvSvid OBJECT-TYPE
			SYNTAX AsDot1adVlanIdOrNone
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"The S-VID to use in the S-Vlan tag inserted on frames as they enter the S-Vlan entity of
				the  provider bridge from the C-Vlan entity.
				"
			REFERENCE
				"IEEE 802.1Q/D11 Section 12.10.1.1"
			DEFVAL { 0 }
			::= { asDot1adIntCvidRegistrationProvEntry 3 }

		
		-- 1.3.6.1.4.1.989.1.16.5.4.2.6.1.4
		asDot1adIntCvidRegistrationProvUserPriority OBJECT-TYPE
			SYNTAX Integer32 (0..7)
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"The user priority to insert into the S-Vlan tag.
				"
			::= { asDot1adIntCvidRegistrationProvEntry 4 }

		
		-- 1.3.6.1.4.1.989.1.16.5.4.2.6.1.5
		asDot1adIntCvidRegistrationProvPepUntagged OBJECT-TYPE
			SYNTAX AsmaxBooleanSwitch
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"Determines whether frames are untagged for the given VLAN on a given port on egress.
				The port referred to here is the internal PEP port that is required when the bridge 
				operates in provider-mode and the SS port is configured for CEP mode.
				The Vlan tag that may be defined as untagged here is the C-Vlan tag."
			DEFVAL { booleanFalse }
			::= { asDot1adIntCvidRegistrationProvEntry 5 }

		
		-- 1.3.6.1.4.1.989.1.16.5.4.2.6.1.6
		asDot1adIntCvidRegistrationProvCepUntagged OBJECT-TYPE
			SYNTAX AsmaxBooleanSwitch
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"Determines whether frames are untagged for the given VLAN on a given port on egress.
				The port referred to here is the external SS port when the bridge 
				operates in provider-mode and the SS port is configured for CEP mode.
				The Vlan tag that may be defined as untagged here is the C-Vlan tag."
			DEFVAL { booleanFalse }
			::= { asDot1adIntCvidRegistrationProvEntry 6 }

		
		-- 1.3.6.1.4.1.989.1.16.5.4.2.6.1.7
		asDot1adIntCvidRegistrationProvRowStatus OBJECT-TYPE
			SYNTAX RowStatus
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"This object indicates the status of this entry."
			::= { asDot1adIntCvidRegistrationProvEntry 7 }

		
		-- 1.3.6.1.4.1.989.1.16.5.4.3
		asDot1adPortClassObjects OBJECT IDENTIFIER ::= { asMaxAdBridge 3 }

		
		-- 1.3.6.1.4.1.989.1.16.5.4.3.1
		asDot1adSsPortProvClassTable OBJECT-TYPE
			SYNTAX SEQUENCE OF AsDot1adSsPortProvClassEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"A table containing the default port provisioning information for all the SS ports allowed 
				on this bridge. 
				
				"
			::= { asDot1adPortClassObjects 1 }

		
		-- 1.3.6.1.4.1.989.1.16.5.4.3.1.1
		asDot1adSsPortProvClassEntry OBJECT-TYPE
			SYNTAX AsDot1adSsPortProvClassEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"One row per air-interface, SS port Class."
			INDEX { ifIndex, asDot1adSsPortProvClassName }
			::= { asDot1adSsPortProvClassTable 1 }

		
		AsDot1adSsPortProvClassEntry ::=
			SEQUENCE { 
				asDot1adSsPortProvClassName
					AsDot1adClassName,
				asDot1adSsPortProvClassAdMode
					AsDot1adPortMode,
				asDot1adSsPortProvClassIngressFilterEnabled
					AsmaxBooleanSwitch,
				asDot1adSsPortProvClassPvid
					AsDot1adVlanIdOrNone,
				asDot1adSsPortProvClassUserPriority
					Integer32,
				asDot1adSsPortProvClassAllowedFrameTypes
					AsDot1adAllowedFrameTypes,
				asDot1adSsPortProvClassRowStatus
					RowStatus,
				asDot1adSsPortProvClassQinqSupported
					AsmaxBooleanSwitch,
				asDot1adSsPortProvClassSTagVlan
					Integer32,
				asDot1adSsPortProvClassUseCTagPriority
					AsmaxBooleanSwitch,
				asDot1adSsPortProvClassSTagPriority
					Integer32
			 }

		-- 1.3.6.1.4.1.989.1.16.5.4.3.1.1.1
		asDot1adSsPortProvClassName OBJECT-TYPE
			SYNTAX AsDot1adClassName
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"The name of the SS Port Class"
			::= { asDot1adSsPortProvClassEntry 1 }

		
		-- 1.3.6.1.4.1.989.1.16.5.4.3.1.1.2
		asDot1adSsPortProvClassAdMode OBJECT-TYPE
			SYNTAX AsDot1adPortMode
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"When the bridge is configured for 'provider' (802.1ad) mode, this parameter indicates 
				the mode the SS port will operate in, or is operating in."
			DEFVAL { portModeCnp }
			::= { asDot1adSsPortProvClassEntry 2 }

		
		-- 1.3.6.1.4.1.989.1.16.5.4.3.1.1.3
		asDot1adSsPortProvClassIngressFilterEnabled OBJECT-TYPE
			SYNTAX AsmaxBooleanSwitch
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"When this is booleanTrue, the device will discard incoming
				frames for VLANs that are not included in this Port's
				egress VLAN list as defined by asDot1adSsPortClassVlanListTable.
				When booleanFalse, the port will accept all incoming frames."
			REFERENCE
				"IEEE 802.1Q/D11 Section 12.10.1.4"
			DEFVAL { booleanFalse }
			::= { asDot1adSsPortProvClassEntry 3 }

		
		-- 1.3.6.1.4.1.989.1.16.5.4.3.1.1.4
		asDot1adSsPortProvClassPvid OBJECT-TYPE
			SYNTAX AsDot1adVlanIdOrNone
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"The PVID, the VLAN-ID assigned to untagged frames or
				Priority-Tagged frames received on this port."
			REFERENCE
				"IEEE 802.1Q/D11 Section 12.10.1.1"
			DEFVAL { 0 }
			::= { asDot1adSsPortProvClassEntry 4 }

		
		-- 1.3.6.1.4.1.989.1.16.5.4.3.1.1.5
		asDot1adSsPortProvClassUserPriority OBJECT-TYPE
			SYNTAX Integer32 (0..7)
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"The user priority of vlan-tagged traffic.
				"
			DEFVAL { 0 }
			::= { asDot1adSsPortProvClassEntry 5 }

		
		-- 1.3.6.1.4.1.989.1.16.5.4.3.1.1.6
		asDot1adSsPortProvClassAllowedFrameTypes OBJECT-TYPE
			SYNTAX AsDot1adAllowedFrameTypes
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"The allowed frame types permitted on incoming traffic to this port.
				When this is admitOnlyVlanTagged, the device will discard 
				untagged frames or Priority-Tagged frames received on this port.
				When admitAll(1), untagged frames or Priority-Tagged frames 
				received on this port will be accepted and assigned to 
				a VID based on the PVID of for this port.
				When admitOnlyUntagged(3), is set the device will discard 
				all VLAN tagged frames."
			REFERENCE
				"IEEE 802.1Q/D11 Section 12.10.1.3"
			DEFVAL { admitOnlyVlanTagged }
			::= { asDot1adSsPortProvClassEntry 6 }

		
		-- 1.3.6.1.4.1.989.1.16.5.4.3.1.1.7
		asDot1adSsPortProvClassRowStatus OBJECT-TYPE
			SYNTAX RowStatus
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"This object indicates the status of this entry."
			::= { asDot1adSsPortProvClassEntry 7 }

		
		-- 1.3.6.1.4.1.989.1.16.5.4.3.1.1.8
		asDot1adSsPortProvClassQinqSupported OBJECT-TYPE
			SYNTAX AsmaxBooleanSwitch
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"QinQ supported for the SS."
			DEFVAL { booleanFalse }
			::= { asDot1adSsPortProvClassEntry 8 }

		
		-- 1.3.6.1.4.1.989.1.16.5.4.3.1.1.9
		asDot1adSsPortProvClassSTagVlan OBJECT-TYPE
			SYNTAX Integer32 (0..4095)
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"sTagVlan."
			::= { asDot1adSsPortProvClassEntry 9 }

		
		-- 1.3.6.1.4.1.989.1.16.5.4.3.1.1.10
		asDot1adSsPortProvClassUseCTagPriority OBJECT-TYPE
			SYNTAX AsmaxBooleanSwitch
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"Use C-Tag priority for S-tag
				."
			DEFVAL { booleanTrue }
			::= { asDot1adSsPortProvClassEntry 10 }

		
		-- 1.3.6.1.4.1.989.1.16.5.4.3.1.1.11
		asDot1adSsPortProvClassSTagPriority OBJECT-TYPE
			SYNTAX Integer32 (0..7)
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"The sTag user priority."
			::= { asDot1adSsPortProvClassEntry 11 }

		
		-- 1.3.6.1.4.1.989.1.16.5.4.3.2
		asDot1adSsPortClassVlanListTable OBJECT-TYPE
			SYNTAX SEQUENCE OF AsDot1adSsPortClassVlanListEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"SS port class egress VLAN lists. It is defined as a map between VLANs 
				and the port class with the tagging behaviour additionally defined.
				Note the provisioning is per air interface, SS Port class and Vlan ID."
			::= { asDot1adPortClassObjects 2 }

		
		-- 1.3.6.1.4.1.989.1.16.5.4.3.2.1
		asDot1adSsPortClassVlanListEntry OBJECT-TYPE
			SYNTAX AsDot1adSsPortClassVlanListEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"One row per air-interface, SS port Class and Vlan ID."
			INDEX { ifIndex, asDot1adSsPortProvClassName, asDot1adSsPortClassVlanListVlanId }
			::= { asDot1adSsPortClassVlanListTable 1 }

		
		AsDot1adSsPortClassVlanListEntry ::=
			SEQUENCE { 
				asDot1adSsPortClassVlanListVlanId
					AsDot1adVlanId,
				asDot1adSsPortClassVlanListUntagged
					AsmaxBooleanSwitch,
				asDot1adSsPortClassVlanListRowStatus
					RowStatus
			 }

		-- 1.3.6.1.4.1.989.1.16.5.4.3.2.1.1
		asDot1adSsPortClassVlanListVlanId OBJECT-TYPE
			SYNTAX AsDot1adVlanId
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"ID of the VLAN member of the port vlan list."
			::= { asDot1adSsPortClassVlanListEntry 1 }

		
		-- 1.3.6.1.4.1.989.1.16.5.4.3.2.1.2
		asDot1adSsPortClassVlanListUntagged OBJECT-TYPE
			SYNTAX AsmaxBooleanSwitch
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"Determines whether frames are untagged for the given VLAN on a given port on egress."
			DEFVAL { booleanFalse }
			::= { asDot1adSsPortClassVlanListEntry 2 }

		
		-- 1.3.6.1.4.1.989.1.16.5.4.3.2.1.3
		asDot1adSsPortClassVlanListRowStatus OBJECT-TYPE
			SYNTAX RowStatus
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"This object indicates the status of this entry."
			::= { asDot1adSsPortClassVlanListEntry 3 }

		
		-- 1.3.6.1.4.1.989.1.16.5.4.4
		asDot1adBridgeConformance OBJECT IDENTIFIER ::= { asMaxAdBridge 4 }

		

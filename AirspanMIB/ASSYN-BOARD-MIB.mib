            DEFVAL { ipv4 }
            ::= { asSynBdNtpServerEntry 2 }

        
        -- 1.3.6.1.4.1.989.1.23.1.6.1.1.3
        asSynBdNtpServerIpAddress OBJECT-TYPE
            SYNTAX InetAddress
            MAX-ACCESS read-create
            STATUS current
            DESCRIPTION

            DEFVAL { "0.0.0.0" }
            ::= { asSynBdNtpServerEntry 3 }

        
        -- 1.3.6.1.4.1.989.1.23.1.6.1.1.4
        asSynBdNtpServerRowStatus OBJECT-TYPE
            SYNTAX RowStatus
            MAX-ACCESS read-create
            STATUS current
            DESCRIPTION
                "Handles Row Creation/ Deletion in the table."
            ::= { asSynBdNtpServerEntry 4 }

        
    
    END


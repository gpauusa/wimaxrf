                "Returns current status of GPS-SB. It includes the alarms information
                ALARMS: for each bit, '0' means alarm condition not active 
                while '1' means alarm condition active
                "
            ::= { asSynBdGpsStsEntry 9 }

        
        -- 1.3.6.1.4.1.989.1.23.1.5.1.1.10
        asSynBdGpsStsModuleType OBJECT-TYPE
            SYNTAX INTEGER { condorGpsModule(0) }
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "Module type as one of the enumerated ones.
                By default, it is assumed as Condor. 
                This enumeration matches definition in IIB."
            ::= { asSynBdGpsStsEntry 10 }

        
        -- 1.3.6.1.4.1.989.1.23.1.5.2
        asSynBdGpsCfgTable OBJECT-TYPE
            SYNTAX SEQUENCE OF AsSynBdGpsCfgEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "Configures GPS configuration."
            ::= { asSynBdGps 2 }

        
        -- 1.3.6.1.4.1.989.1.23.1.5.2.1
        asSynBdGpsCfgEntry OBJECT-TYPE
            SYNTAX AsSynBdGpsCfgEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "GPS configuration."
            INDEX { asSynBdGpsInstance }
            ::= { asSynBdGpsCfgTable 1 }

        
        AsSynBdGpsCfgEntry ::=
            SEQUENCE { 
                asSynBdGpsCfgResetGps
                    AsCmnTcBooleanSwitch
             }

        -- 1.3.6.1.4.1.989.1.23.1.5.2.1.1
        asSynBdGpsCfgResetGps OBJECT-TYPE
            SYNTAX AsCmnTcBooleanSwitch
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Set to TRUE to force reset of GPS module."
            DEFVAL { booleanFalse }
            ::= { asSynBdGpsCfgEntry 1 }

        
        -- 1.3.6.1.4.1.989.1.23.1.6
        asSynBdMisc OBJECT IDENTIFIER ::= { asSynBd 6 }

        
        -- 1.3.6.1.4.1.989.1.23.1.6.1
        asSynBdNtpServerTable OBJECT-TYPE
            SYNTAX SEQUENCE OF AsSynBdNtpServerEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "Table containing the NTP Server definitions to be configured on the basestation."
            ::= { asSynBdMisc 1 }

        
        -- 1.3.6.1.4.1.989.1.23.1.6.1.1
        asSynBdNtpServerEntry OBJECT-TYPE
            SYNTAX AsSynBdNtpServerEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "Description."
            INDEX { asSynBdNtpServerId }
            ::= { asSynBdNtpServerTable 1 }

        
        AsSynBdNtpServerEntry ::=
            SEQUENCE { 
                asSynBdNtpServerId
                    Integer32,
                asSynBdNtpServerIpAddressType
                    InetAddressType,
                asSynBdNtpServerIpAddress
                    InetAddress,
                asSynBdNtpServerRowStatus
                    RowStatus
             }

        -- 1.3.6.1.4.1.989.1.23.1.6.1.1.1
        asSynBdNtpServerId OBJECT-TYPE
            SYNTAX Integer32 (1..2147483647)
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "Arbitrary index for entry in NTP server table. "
            ::= { asSynBdNtpServerEntry 1 }

        
        -- 1.3.6.1.4.1.989.1.23.1.6.1.1.2
        asSynBdNtpServerIpAddressType OBJECT-TYPE
            SYNTAX InetAddressType
            MAX-ACCESS read-create
            STATUS current
            DESCRIPTION

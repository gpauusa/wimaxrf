        wmanIf2mBsOfdmaBandAMCRetryTimer        INTEGER,
        wmanIf2mBsOfdmaHandoverRangingStart     INTEGER,
        wmanIf2mBsOfdmaHandoverRangingEnd       INTEGER,
        wmanIf2mBsOfdmaHARQAackDelayDLBurst     WmanIf2mHarqAckDelay,
        wmanIf2mBsOfdmaUlAmcAlloPhyBandsBitmap  OCTET STRING,
        wmanIf2mBsOfdmaMaxRetransmission        INTEGER,
        wmanIf2mBsOfdmaNormalizedCnOverride     OCTET STRING,
        wmanIf2mBsOfdmaSizeOfCqichId            INTEGER,

    necWimaxBsSwSide0Ver OBJECT-TYPE
        SYNTAX              OCTET STRING (SIZE(0..32))
        MAX-ACCESS          read-only
        STATUS              current
        DESCRIPTION         "side 0 software version"
        ::= { necWimaxBsSwAdminEntry 5 }

    necWimaxBsSwSide1Ver OBJECT-TYPE
        SYNTAX              OCTET STRING (SIZE(0..32))
        MAX-ACCESS          read-only
        STATUS              current
        DESCRIPTION         "side 1 software version"
        ::= { necWimaxBsSwAdminEntry 6 }

    necWimaxBsOduSwSwichover OBJECT-TYPE
        SYNTAX              INTEGER {
                                noSwichOver( 0 ),
                                swichOver( 1 )
                            }
        MAX-ACCESS          read-write
        STATUS              current
        DESCRIPTION         "When this value is set to 1(swich over), ODU sw
                            specified in necWimaxBsSwRollback is swiched."
        ::= { necWimaxBsSoftwareAdmin 2 }


                                externalIFDeviceProblem( 54 ),
                                lineCardProblem( 55 ),
                                multiplexerProblem( 56 ),
                                nEIdentifierDuplication( 57 ),
                                powerProblem( 58 ),
                                processorProblem( 59 ),
                                protectionPathFailure( 60 ),

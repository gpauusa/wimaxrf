    necWimaxBsEventType OBJECT-TYPE
        SYNTAX              INTEGER {
                                alarm( 0 ),
                                event( 1 )
                            }
        MAX-ACCESS          read-only
        STATUS              current
        DESCRIPTION         "alarm / event"
        ::= { necWimaxBsEventEntry 2 }

    necWimaxBsEventLogLevel OBJECT-TYPE
        SYNTAX              NecWimaxBsEventLogLevel
        MAX-ACCESS          read-write
        STATUS              current
        DESCRIPTION         "This object describes the severity of such event.
                            The system will assign a severity for each event.
                            But, it can be configurable by NMS"
        ::= { necWimaxBsEventEntry 3 }

    necWimaxBsEventNotification OBJECT-TYPE
        SYNTAX              TruthValue
        MAX-ACCESS          read-write
        STATUS              current
        DESCRIPTION         "An event notification will be reported when it
                            is True (1)."
        ::= { necWimaxBsEventEntry 4 }

    necWimaxBsEventDescription OBJECT-TYPE
        SYNTAX              OCTET STRING (SIZE(0..127))
        MAX-ACCESS          read-write
        STATUS              current
        DESCRIPTION         "This object describes the event."
        ::= { necWimaxBsEventEntry 5 }

    necWimaxBsAlarmSeverity OBJECT-TYPE
        SYNTAX              NecWimaxBsAlarmSeverity
        MAX-ACCESS          read-write
        STATUS              current
        DESCRIPTION         "This object describes the severity of such event.
                            The system will assign a severity for each event.
                            But, it can be configurable by NMS"
        ::= { necWimaxBsEventEntry 6 }

    necWimaxBsAlarmType OBJECT-TYPE
        SYNTAX              NecWimaxBsAlarmType
        MAX-ACCESS          read-write
        STATUS              current
        DESCRIPTION         "Alarm types per X.733/X.736."
        ::= { necWimaxBsEventEntry 7 }

    necWimaxBsAlarmProbableCause OBJECT-TYPE
        SYNTAX              NecWimaxBsAlarmProbableCause
        MAX-ACCESS          read-write
        STATUS              current
        DESCRIPTION         "Probable causes per X.733/X.736. Values 1..50
                            are used with communications alarms. Values 51..100
                            are used with equipment alarms. Values 101..150
                            are used with environmental alarms. Values 151..200
                            are used with processing error alarms. Values
                            501..600 are used with any of the violation alarm
                            types."
        ::= { necWimaxBsEventEntry 8 }

    necWimaxBsAlarmRestart OBJECT-TYPE
        SYNTAX              INTEGER {

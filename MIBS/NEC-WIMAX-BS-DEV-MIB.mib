    necWimaxBsMonitorUdpmode OBJECT-TYPE
        SYNTAX              BITS {
                                asngwMng( 0 ),
                                asngwData( 1 ),
                                msMngSdu( 2 ),
                                msDataSdu( 3 ),
                                msPdu( 4 )
                            }
        MAX-ACCESS          read-write
        STATUS              current

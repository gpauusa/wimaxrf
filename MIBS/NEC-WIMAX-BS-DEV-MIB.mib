        MAX-ACCESS          not-accessible
        STATUS              current
        DESCRIPTION         "software management"
        ::= { necWimaxBsSoftwareAdmin 1 }

    necWimaxBsSwAdminEntry OBJECT-TYPE
        SYNTAX              NecWimaxBsSwAdminEntry
        MAX-ACCESS          not-accessible
        STATUS              current
        DESCRIPTION         "Software Management Table. Index 1 = MAC/NWINFC,
                            2 = MAC PHY/NWINFC GPS, 3 = ODU#1(DSP), 4 = ODU#1(DPD
                            FPGA), 5 = ODU#1(ODU Parameter), 6 = ODU#1(IDU
                            INTF FPCGA), 7 = ODU#1(PLD)"
        INDEX               {
                                necWimaxBsDeviceIndex
                            }
        ::= { necWimaxBsSwAdminTable 1 }

    NecWimaxBsSwAdminEntry ::= SEQUENCE {
        necWimaxBsSwActiveVer
            OCTET STRING,
        necWimaxBsSwBackupVer
            OCTET STRING,

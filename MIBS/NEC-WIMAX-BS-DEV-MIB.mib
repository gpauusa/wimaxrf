    necWimaxBsThresholdConfigTable OBJECT-TYPE
        SYNTAX              SEQUENCE OF NecWimaxBsThresholdConfigEntry
        MAX-ACCESS          not-accessible
        STATUS              current
        DESCRIPTION         "This table contains threshold objects that can
                            be set to detect the threshold crossing events."
        ::= { necWimaxBsTrapControl 2 }

    necWimaxBsThresholdConfigEntry OBJECT-TYPE
        SYNTAX              NecWimaxBsThresholdConfigEntry
        MAX-ACCESS          not-accessible
        STATUS              current
        DESCRIPTION         "This table provides one row for each BS sector,
                            and is indexed by wmanPriDeviceIndex."
        INDEX               {
                                necWimaxBsDeviceIndex
                            }
        ::= { necWimaxBsThresholdConfigTable 1 }

    NecWimaxBsThresholdConfigEntry ::= SEQUENCE {
        necWimaxBsTempLowAlarmThreshold
            Integer32,
        necWimaxBsTempLowAlarmRestoredThreshold
            Integer32,
        necWimaxBsTempHighAlarmThreshold
            Integer32,
        necWimaxBsTempHighAlarmRestoredThreshold
            Integer32
    }

    necWimaxBsTempLowAlarmThreshold OBJECT-TYPE
        SYNTAX              Integer32 (-128..127)
        UNITS               "degreeC"
        MAX-ACCESS          read-write
        STATUS              current
        DESCRIPTION         "Low threshold for generating the temperature low
                            alarm trap. The detection of temperature low
                            alarm will be disabled until the temperature
                            goes above wmanPriTempLowAlarmRestoredThreshold"
        ::= { necWimaxBsThresholdConfigEntry 2 }

    necWimaxBsTempLowAlarmRestoredThreshold OBJECT-TYPE
        SYNTAX              Integer32 (-128..127)
        UNITS               "degreeC"
        MAX-ACCESS          read-write
        STATUS              current
        DESCRIPTION         "Low threshold for generating a trap indicating
                            the temperature alarm is restored."
        ::= { necWimaxBsThresholdConfigEntry 3 }

    necWimaxBsTempHighAlarmThreshold OBJECT-TYPE
        SYNTAX              Integer32 (-128..127)
        UNITS               "degreeC"
        MAX-ACCESS          read-write
        STATUS              current
        DESCRIPTION         "Low threshold for generating the temperature low
                            alarm trap. The detection of temperature low
                            alarm will be disabled until the temperature
                            goes above wmanPriTempLowAlarmRestoredThreshold"
        ::= { necWimaxBsThresholdConfigEntry 4 }

    necWimaxBsTempHighAlarmRestoredThreshold OBJECT-TYPE
        SYNTAX              Integer32 (-128..127)
        UNITS               "degreeC"
        MAX-ACCESS          read-write
        STATUS              current
        DESCRIPTION         "High threshold for generating a trap indicating
                            the temperature alarm is restored."
        ::= { necWimaxBsThresholdConfigEntry 5 }

    necWimaxBsDevBSID OBJECT-TYPE
        SYNTAX              OCTET STRING (SIZE(6))
        MAX-ACCESS          read-write
        STATUS              current
        DESCRIPTION         "It is represented ID as for a physical device
